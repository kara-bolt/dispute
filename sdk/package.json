{
  "name": "@kara/dispute-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for KaraDispute - AI-arbitrated dispute resolution on Base",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "kara",
    "dispute",
    "arbitration",
    "ai",
    "escrow",
    "base",
    "ethereum",
    "web3",
    "agents"
  ],
  "author": "Kara <hello@kara.finance>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kara-bolt/dispute"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "viem": "^2.45.1",
    "vitest": "^1.0.0"
  }
}
